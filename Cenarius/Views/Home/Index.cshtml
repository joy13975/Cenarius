<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8" />
    <title>Cenarius</title>
    <!-- css deps -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/css/bootstrap.min.css" />
    <link rel="stylesheet" href="../../Libraries/CenariusForm/CenariusForm.css" />
    <!-- js deps -->
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="https://cdn.rawgit.com/google/code-prettify/master/loader/run_prettify.js"></script>
</head>

<body class="index-body"
      id="bootstrap-overrides">
    <div class="panel panel-default formo-uploader">
        <div class="panel-heading">
            <font style="text-align: center">
                <h2>
                    <b>
                        Upload New Formo
                    </b>
                </h2>
            </font>
        </div>
        <form action="~/Home/UploadFormo" method="post" enctype='multipart/form-data'>
            <div class="panel-body">
                <div class="container-fluid" style="margin-top: 40px; margin-left: 30px;">
                    <div class="row">
                        <input type="file" name="formoFile" accept=".json" />
                        <button type="submit" class="btn btn-primary" style="float: right; margin-top: -30px; margin-right: 30px">
                            <span class="glyphicon glyphicon-cloud-upload" style="font-size: 25px"></span>
                            <font size="5">Submit</font>
                        </button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</body>
</html>