{
    "forma": {
        "basic_info": {
            "_title": "基本資料",
            "_exclude_from_summary": true,
            "_properties": {
                "patient": {
                    "_title": "受檢者",
                    "_default_cols": 4,
                    "_properties": {
                        "patient_id": {
                            "_title": "病歷號",
                            "_default_value": "A1337"
                        },
                        "patient_name": {
                            "_title": "姓名",
                            "_default_value": "王小明"
                        },
                        "patient_no_of_checks_today": {
                            "_title": "今日檢查次數",
                            "_ending": "次",
                            "_type": "integer",
                            "_min": 0,
                            "_default_value": 1
                        }
                    }
                },
                "current_checkup": {
                    "_title": "本次檢查",
                    "_default_cols": 4,
                    "_properties": {
                        "report_date": {
                            "_title": "日期",
                            "_type": "date"
                        },
                        "report_id": {
                            "_title": "編號 ",
                            "_default_value": "B12938"
                        },
                        "report_no_of_checks": {
                            "_title": "次數 ",
                            "_ending": "次",
                            "_type": "integer",
                            "_min": 0,
                            "_default_value": 1
                        },
                        "operator_id": {
                            "_title": "操作者編號",
                            "_default_value": "C000301"
                        },
                        "interpreter_id": {
                            "_title": "判讀者編號",
                            "_default_value": "D000010"
                        },
                        "machine_id": {
                            "_title": "檢查機器編號 ",
                            "_default_value": "N/A"
                        },
                        "us_examination_mode": {
                            "_title": "US Examination Mode",
                            "_cols": 8,
                            "_enum": [
                                "Screening US",
                                "Diagnostic US",
                                "Both"
                            ],
                            "_default_value": 1
                        }
                    }
                }
            }
        },
        "indications": {
            "_title": "檢查適應症",
            "_default_type": "boolean",
            "_exclude_from_summary": true,
            "_properties": {
                "00_asymptomatic_screening": {
                    "_title": "00. Asymptomatic Screening",
                    "_cols": 12,
                    "_default_value": true
                },
                "10_exam_for_symptomatic_patient": {
                    "_title": "10. Exam for symptomatic patient",
                    "_cols": 12,
                    "_default_cols": 4,
                    "_html_class": "tight",
                    "_enum_multi": [
                        "11. Pain",
                        "12. Palpable mass",
                        "13. Discharge"
                    ]
                },
                "20_screening_mammographic_category": {
                    "_title": "20. Screening Mammographic Category (0-5)",
                    "_cols": 12,
                    "_enum": [
                        0,
                        1,
                        2,
                        3,
                        4,
                        5
                    ]
                },
                "benign_lesions": {
                    "_title": "Benign Lesion(s)",
                    "_type": "subobject",
                    "_default_type": "boolean",
                    "_default_cols": 4,
                    "_properties": {
                        "test_so_lv2": {
                            "_type": "subobject",
                            "_properties": {
                                "fill_me": {
                                    "_default_value": "よかった"
                                },
                                "fill_me2": {
                                    "_type": "float",
                                    "_default_value": 1337.9
                                }
                            }
                        },
                        "location": {
                            "_default_type": "boolean",
                            "_cols": 8,
                            "_grouping": "either",
                            "_properties": {
                                "general": {
                                    "_properties": {
                                        "side": {
                                            "_enum": [
                                                "Right",
                                                "Left"
                                            ]
                                        },
                                        "quarter": {
                                            "_enum": [
                                                "UOQ",
                                                "UIQ",
                                                "LOQ",
                                                "LIQ"
                                            ]
                                        }
                                    }
                                },
                                "specific": {
                                    "_properties": {
                                        "angle": {
                                            "_type": "float",
                                            "_ending": "o'clock",
                                            "_number_step": 0.1
                                        },
                                        "distance": {
                                            "_type": "float",
                                            "_ending": "cm"
                                        }
                                    }
                                }
                            }
                        },
                        "mass": {
                            "_title": "21. Mass"
                        },
                        "calcification": {
                            "_title": "22. Calcification",
                            "_default_value": true
                        },
                        "asymmetry": {
                            "_title": "23. Asymmetry"
                        },
                        "architecture_distortion": {
                            "_title": "24. Architecture distortion"
                        },
                        "others": {
                            "_title": "25. Others",
                            "_type": "string",
                            "_force_checkbox": true,
                            "_cols": 12
                        },
                        "density_of_breast": {
                            "_title": "26. Density of breast",
                            "_cols": 6,
                            "_force_checkbox": true,
                            "_enum": [
                                "A",
                                "B",
                                "C",
                                "D"
                            ],
                            "_default_value": 2
                        },
                        "recommendation": {
                            "_title": "27. Recomendation",
                            "_type": "string",
                            "_force_checkbox": true,
                            "_cols": 6
                        }
                    }
                },
                "30_f_u_for_previous_noted_benign_lesions": {
                    "_title": "30. F/U for previous noted benign lesions",
                    "_default_type": "boolean",
                    "_properties": {
                        "fibroadenoma": {
                            "_title": "31. Fibroadenoma",
                            "_default_value": true
                        },
                        "fibrocystic_change": {
                            "_title": "32. Fibrocystic change"
                        },
                        "cyst": {
                            "_title": "33. Cyst"
                        },
                        "other": {
                            "_title": "34. Other",
                            "_type": "string",
                            "_force_checkbox": true,
                            "_default_value": "not sure"
                        }
                    }
                },
                "40_surgically_altered_breast": {
                    "_title": "40. Surgically altered breast",
                    "_cols": 12,
                    "_enum_multi": [
                        "41. S/P biopsy/wide excision/BCS",
                        "42. S/P breast augmentation/Reconstruction"
                    ]
                },
                "50_other_reasons_for_breast_sono_examination": {
                    "_title": "50. Other reasons for breast sono examination",
                    "_cols": 12
                }
            }
        },
        "interpretation": {
            "_exclude_from_summary": true,
            "_properties": {
                "us_category": {
                    "_title": "US Category 分類",
                    "_cols": 4,
                    "_enum": [
                        "0. Category 0",
                        "1. Category 1",
                        "2. Category 2",
                        "3. Category 3",
                        "4A. Category 4A",
                        "4B. Category 4B",
                        "4C. Category 4C",
                        "5. Category 5",
                        "6. Category 6"
                    ]
                },
                "combined_mammogram_and_us_final_category": {
                    "_title": "Combined Mammogram And US Final Category",
                    "_cols": 8,
                    "_enum": [
                        "0.Category 0",
                        "1.Category 1",
                        "2.Category 2",
                        "3.Category 3",
                        "4A.Category 4A",
                        "4B.Category 4B",
                        "4C.Category 4C",
                        "5.Category 5",
                        "6.Category 6"
                    ]
                },
                "biopsy_and_other_evaluation_suggestion": {
                    "_default_cols": 3,
                    "_enum": [
                        {
                            "_title": "0. No need biopsy",
                            "_default_value": true
                        },
                        "1. Aspiration cytology",
                        "2. Core biopsy",
                        "3. Open biopsy",
                        "4. Mammography",
                        "5. MRI", {
                            "_title": "6. Other",
                            "_type": "string",
                            "_cols": 6
                        }
                    ]
                },
                "pathology_results": {
                    "_cols": 12,
                    "_enum": [
                        "0. 未做biopsy",
                        "1. Benign",
                        "2. Malignant-Carcinoma in situ",
                        "3. Maligant-Invasive cancer",
                        "4. Malignant-other",
                        "5. Cytology: suspicious",
                        "6. Cytology: atypia",
                        "7. Cytology: positive",
                        "8. Cytology: negative",
                        "9. 已做biopsy,結果不知"
                    ]
                }
            }
        },
        "tumour_details": {
            "_title": "超音波腫瘤特性描述",
            "_help_text": "BI-RADS 4以上均需記錄。若BI-RADS 3之病灶數目大於或等於四個，則病灶特性描述至少需記錄4個，若少於四個，則全部記錄。",
            "_type": "subobject",
            "_properties": {
                "side": {
                    "_title": "左右側",
                    "_enum": [
                        "L (左)",
                        "R (右)"
                    ]
                },
                "position": {
                    "_title": "方位",
                    "_ending": "o'clock",
                    "_type": "integer",
                    "_default_value": 2
                },
                "distance": {
                    "_title": "距離乳頭距離",
                    "_type": "float",
                    "_ending": "cm",
                    "_default_value": 3.5
                },
                "max_diameter": {
                    "_title": "最大徑",
                    "_type": "float",
                    "_ending": "mm",
                    "_default_value": 5
                },
                "shape": {
                    "_enum": [
                        "1. Oval",
                        "2. Round",
                        "3. Irregular"
                    ]
                },
                "orientation": {
                    "_enum": [
                        "1. Parallel",
                        "2. Not parallel"
                    ]
                },
                "margin": {
                    "_enum": [
                        "1.Circumscribed",
                        "2.Indistinct",
                        "3.Angular",
                        "4.Microlobulated",
                        "5.Spiculated"
                    ]
                },
                "echo_pattern": {
                    "_cols": 12,
                    "_enum": [
                        "1. Anechoic",
                        "2. Hyperechoic",
                        "3. Complex",
                        "4. Hypoechoic",
                        "5. Isoechoic"
                    ]
                },
                "posterior_features": {
                    "_cols": 12,
                    "_enum": [
                        "1. No posterior acoustic features",
                        "2. Enhancement",
                        "3. Shadowing",
                        "4. Combined pattern"
                    ]
                },
                "associated_features": {
                    "_default_cols": 4,
                    "_summary_break_style": "before",
                    "_enum_multi": [
                        "1. Architectural distortion",
                        "2. Duct Changes ",
                        "3. Skin changes/Skin Thickening", {
                            "_title": "4. Skin changes/Skin Retraction",
                            "_cols": 8,
                            "_default_value": true
                        },
                        "5. Edema"
                    ]
                },
                "vascularity": {
                    "_cols": 12,
                    "_enum": [
                        "0. Not assessed",
                        "1. Vascularity/Absent",
                        "2. Vascularity/Internal Vascularity",
                        "3. Vascularity/Vessels in Rim"
                    ]
                },
                "surrounding_change": {
                    "_default_cols": 4,
                    "_enum_multi": [
                        "1. Duct changes",
                        "2. Cooper’s ligament changes", {
                            "_title": "3. Edema",
                            "_default_value": true
                        },
                        "4. Architectural distortion",
                        "5. Skin thickening",
                        "6. Skin retraction/irregularity"
                    ]
                },
                "special_case": {
                    "_default_cols": 4,
                    "_enum_multi": [
                        "1. Clustered microcysts",
                        "2. Complicated cysts",
                        "3. Mass in or on skin",
                        "4. Foreign body",
                        "5. Simple cyst", {
                            "_title": "6. VascularAbnormalities/AVM (Arteriovenousmalformations/pseudoaneurysms)",
                            "_cols": 12,
                            "_default_value": true
                        }, {
                            "_title": "7. VascularAbnormalities/Mondor Disease",
                            "_cols": 8
                        }, {
                            "_title": "8. Postsurgical Fluid Collection",
                            "_cols": 8
                        },
                        "9. Fat Necrosis", {
                            "_title": "10. Lymph nodes – intramammary",
                            "_cols": 8
                        },
                        "11. Lymph nodes – axillary"
                    ]
                },
                "calcifications": {
                    "_enum": [
                        "1. Intraductal",
                        "2. Microcalcifications out of mass",
                        "3. Microcalcifications in mass"
                    ]
                },
                "elasticity_assessment": {
                    "_enum": [
                        "0. Not assessed",
                        "1. Soft",
                        "2. Intermediate",
                        "3. Hard"
                    ]
                },
                "tumour_summary": {
                    "_title": "檢查小結",
                    "_type": "big_string",
                    "_cols": 12,
                    "_default_value": "What is life?",
                    "_summary_break_style": "before"
                }
            }
        },
        "overall_summary": {
            "_title": "報告總結",
            "_type": "big_string",
            "_max_string_length": 8096,
            "_cols": 12,
            "_default_value": "Life is about finding happiness."
        }
    }
}